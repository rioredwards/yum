{
  "name": "yum",
  "colors": {
    "focusBorder": "highlightActive1",
    "foreground": "txtDefault",
    "disabledForeground": "txtInactive",
    "selection.background": "highlightActive2",
    "descriptionForeground": "txtInactive",
    "icon.foreground": "txtDefault",

    "textLink.foreground": "txtSelected",
    "textLink.activeForeground": "txtLinkActive",

    "toolbar.hoverBackground": "highlightInActive1",
    "toolbar.activeBackground": "highlightActive1",

    "editor.foreground": "txtDefault",
    "editor.background": "bgDark2",
    "editorGutter.background": "bgLight1",
    "editor.lineHighlightBackground": "highlightActive4",
    "editorWidget.background": "bgLight1",
    "editorSuggestWidget.selectedBackground": "highlightActive2",
    "editorSuggestWidget.highlightForeground": "highlightAlt1",
    "editorHoverWidget.border": "bgLight3",
    "editorHoverWidget.background": "bgLight1",
    "editorHoverWidget.foreground": "txtDefault",
    "editorStickyScroll.background": "bgDark2",

    "editorBracketMatch.border": "tkBracketHighlight1",
    "editorBracketHighlight.foreground1": "tkBracketColor1",
    "editorBracketHighlight.foreground2": "tkBracketColor2",
    "editorBracketHighlight.foreground3": "tkBracketColor3",
    "editorBracketHighlight.foreground4": "tkBracketColor4",
    "editorBracketHighlight.foreground5": "tkBracketColor5",
    "editorBracketHighlight.foreground6": "tkBracketColor6",
    "editorBracketHighlight.unexpectedBracket.foreground": "tkFalse",

    "activityBar.background": "bgLight2",
    "activityBar.foreground": "txtSelected",
    "activityBar.activeBackground": "bgLight3",
    "activityBar.inactiveForeground": "txtInactive",
    "activityBarBadge.background": "txtSelected",
    "activityBarBadge.foreground": "txtLight",

    "titleBar.activeBackground": "bgLight2",
    "titleBar.activeForeground": "txtDefault",
    "titleBar.inactiveBackground": "bgLight1",
    "titleBar.inactiveForeground": "txtDefault",

    "input.background": "bgDark2",
    "input.foreground": "txtDefault",
    "input.border": "highlightActive2",
    "input.placeholderForeground": "txtInactive",
    "inputOption.activeBackground": "highlightActive2",
    "inputOption.activeBorder": "highlightActive2",
    "inputOption.hoverBackground": "highlightInActive2",

    "button.background": "highlightActive2",
    "button.foreground": "txtLight",
    "button.hoverBackground": "highlightActive1",

    "badge.background": "highlightInActive1",

    "list.activeSelectionBackground": "highlightActive2",
    "list.activeSelectionForeground": "txtLight",
    "list.inactiveSelectionBackground": "highlightInActive2",
    "list.focusBackground": "highlightActive3",
    "list.focusOutline": "transparent",
    "list.hoverBackground": "highlightInactive3",

    "scrollbarSlider.activeBackground": "highlightActive2",
    "scrollbarSlider.hoverBackground": "highlightActive3",
    "scrollbarSlider.background": "highlightInActive2",

    "sideBar.background": "bgDark2",
    "sideBar.foreground": "txtDefault",
    "sideBar.border": "bgLight2",
    "sideBarSectionHeader.background": "bgLight2",

    "statusBar.foreground": "txtDefault",
    "statusBar.background": "bgLight2",
    "statusBarItem.hoverBackground": "highlightInActive2",

    "tab.inactiveBackground": "bgLight1",
    "tab.border": "bgLight2",
    "tab.activeBackground": "highlightActive3",
    "tab.activeBorder": "highlightActive2",

    "quickInput.background": "bgLight1",
    "quickInputList.focusBackground": "highlightActive2",
    "quickInputList.focusForeground": "txtLight",
    "quickInput.foreground": "txtDefault",

    "panel.background": "bgDark1",
    "panelTitle.activeForeground": "txtDefault"
  },
  "tokenColors": [
    /* General */
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "tkGrey1"
      }
    },
    {
      "name": "colors",
      "scope": [
        "constant.numeric.hex",
        "constant.rgb-value",
        "constant.other.color",
        "meta.function.color"
      ],
      "settings": {
        "foreground": "tkTurquoise1"
      }
    },
    {
      "name": "emphasis",
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "strong",
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "tkError"
      }
    },
    {
      "name": "true",
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "tkTrue"
      }
    },
    {
      "name": "false",
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "tkFalse"
      }
    },
    {
      "name": "punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "tkYellowGreen1"
      }
    },
    {
      "name": "string special tokens",
      "scope": [
        "constant.character.escape",
        "constant.regexp",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "tkBabyBlue1"
      }
    },
    {
      "name": "constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "tkRedOrange2"
      }
    },
    {
      "name": "nullish values",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "tkUndefined"
      }
    },
    {
      "name": "numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "tkGreen1"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    {
      "name": "Variables-readwrite",
      "scope": ["variable.other.readwrite"],
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    {
      "name": "Objects",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "tkOrangeYellow2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object/Variable properties",
      "scope": [
        "variable.other.property",
        "meta.object.member",
        "meta.object-literal.key",
        "meta.var.expr",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "tkRed1"
      }
    },
    {
      "name": "storage: declarations (class, function, int, var, ect...)",
      "scope": ["storage"],
      "settings": {
        "foreground": "tkGreen2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "tkTurquoise1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "storage modifier (static, final, abstract, async, ect...)",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "tkIndigo1"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.operator.expression.typeof"],
      "settings": {
        "foreground": "tkIndigo1"
      }
    },
    {
      "name": "Export & Import",
      "scope": ["keyword.control.export", "keyword.control.import"],
      "settings": {
        "foreground": "tkTeal1"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "entity",
      "scope": ["entity.name.selector"],
      "settings": {
        "foreground": "tkFalse"
      }
    },
    {
      "name": "meta",
      "scope": ["meta"],
      "settings": {
        "foreground": "tkWhite2"
      }
    },
    // JAVASCRIPT ----------------------------------------
    {
      "scope": "string.quoted.double.js",
      "settings": {
        "foreground": "tkGreen2"
      }
    },
    // TYPESCRIPT ----------------------------------------
    {
      "name": "Typescript: Basic Types",
      "scope": ["support.type.primitive.ts"],
      "settings": {
        "foreground": "tkBlue1"
      }
    },
    {
      "name": "Typescript: Built-in Types",
      "scope": ["support.type.builtin.ts"],
      "settings": {
        "foreground": "tkBlue1"
      }
    },
    {
      "name": "Typescript: User-Defined Types + Type Aliases",
      "scope": ["entity.name.type.ts", "entity.name.type.alias.ts"],
      "settings": {
        "foreground": "tkBabyBlue1"
      }
    },
    {
      "name": "Typescript: Interfaces, Enums",
      "scope": ["entity.name.type.interface.ts", "entity.name.type.enum.ts"],
      "settings": {
        "foreground": "tkTurquoise2"
      }
    },
    {
      "name": "Typescript: Classes",
      "scope": [
        "entity.name.type.class.ts",
        "entity.other.inherited-class.ts",
        "support.class.ts"
      ],
      "settings": {
        "foreground": "tkTurquoise1"
      }
    },
    {
      "name": "Typescript: New",
      "scope": ["keyword.operator.new.ts"],
      "settings": {
        "foreground": "tkTrue"
      }
    },
    {
      "name": "Typescript: Delete",
      "scope": ["keyword.operator.expression.delete.ts"],
      "settings": {
        "foreground": "tkFalse"
      }
    },
    {
      "name": "Typescript: Type Checks",
      "scope": [
        "keyword.operator.expression.instanceof.ts",
        "keyword.operator.expression.typeof.ts",
        "keyword.operator.expression.in.ts",
        "keyword.operator.expression.keyof.ts"
      ],
      "settings": {
        "foreground": "tkViolet1"
      }
    },
    // REACT.JS ----------------------------------------
    {
      "scope": "entity.name.tag.js",
      "settings": {
        "foreground": "tkRedOrange2"
      }
    },
    {
      "scope": "support.class.component.js",
      "settings": {
        "foreground": "tkRed1"
      }
    },
    {
      "scope": "entity.other.attribute-name.js",
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    // HTML ----------------------------------------
    {
      "scope": "string.quoted.double.html",
      "settings": {
        "foreground": "tkYellowGreen1"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "tkRedOrange2"
      }
    },
    {
      "name": "HTML punctuation",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "tkTeal2"
      }
    },
    {
      "name": "HTML attribute",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    // CSS ----------------------------------------
    {
      "name": "CSS Selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    {
      "name": "CSS properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css"
      ],
      "settings": {
        "foreground": "tkTurquoise1"
      }
    },
    {
      "name": "CSS functions",
      "scope": [
        "source.css support.function.transform",
        "source.css support.function.timing-function",
        "source.css support.function.misc"
      ],
      "settings": {
        "foreground": "tkGreen1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS property values",
      "scope": [
        "support.property-value",
        "constant.numeric.css",
        "constant.rgb-value",
        "support.property-value.scss",
        "constant.rgb-value.scss",
        "meta.property-value.css",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "tkWhite2"
      }
    },
    {
      "name": "CSS units",
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "tkRed1"
      }
    },
    {
      "name": "CSS variables",
      "scope": ["variable.css", "variable.argument.css"],
      "settings": {
        "foreground": "tkRedOrange1"
      }
    },
    {
      "name": "CSS Important",
      "scope": ["keyword.other.important.css"],
      "settings": {
        "foreground": "tkTrue",
        "fontStyle": "bold"
      }
    },
    // JSON ----------------------------------------
    {
      "scope": "string.quoted.double.json",
      "settings": {
        "foreground": "tkWhite2"
      }
    },
    {
      "name": "JSON Key - Level 0, 4, 8",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "tkTeal2"
      }
    },
    {
      "name": "JSON Key - Level 1, 5",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    {
      "name": "JSON Key - Level 2, 6",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "tkTeal1"
      }
    },
    {
      "name": "JSON Key - Level 3, 7",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "tkRedOrange1"
      }
    },
    // MARKDOWN/MARKUP ----------------------------------------
    {
      "name": "markup",
      "scope": ["markup"],
      "settings": {
        "foreground": "tkWhite2"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "tkWhite1",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "tkViolet1"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "tkRedOrange2"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "tkOrangeYellow2"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "tkGreen1"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "tkRed1"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "tkRedOrange2"
      }
    },
    {
      "scope": "markup.punctuation.quote.beginning",
      "settings": {
        "foreground": "tkBabyBlue1"
      }
    },
    {
      "scope": "markup.punctuation.list.beginning",
      "settings": {
        "foreground": "tkBabyBlue1"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "tkTeal2"
      }
    }
  ]
}
