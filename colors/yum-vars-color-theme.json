{
    "name": "yum",
    "colors": {
        "focusBorder": "highlightActive1",
        "foreground": "txtDefault",
        "disabledForeground": "txtInactive",
        "selection.background": "highlightActive2",
        "descriptionForeground": "txtInactive",
        "icon.foreground": "txtDefault",

        "textLink.foreground": "txtSelected",
        "textLink.activeForeground": "txtLinkActive",

        "toolbar.hoverBackground": "highlightInActive1",
        "toolbar.activeBackground": "highlightActive1",

        "editor.foreground": "txtDefault",
        "editor.background": "bgDark2",
        "editorGutter.background": "bgLight1",
        "editor.lineHighlightBackground": "highlightActive4",
        "editorWidget.background": "bgLight1",
        "editorSuggestWidget.selectedBackground": "highlightActive2",
        "editorSuggestWidget.highlightForeground": "highlightAlt1",
        "editorHoverWidget.border": "bgLight3",
        "editorHoverWidget.background": "bgLight1",
        "editorHoverWidget.foreground": "txtDefault",
        "editorStickyScroll.background": "bgDark2",

        "activityBar.background": "bgLight2",
        "activityBar.foreground": "txtSelected",
        "activityBar.activeBackground": "bgLight3",
        "activityBar.inactiveForeground": "txtInactive",
        "activityBarBadge.background": "txtSelected",
        "activityBarBadge.foreground": "txtLight",

        "titleBar.activeBackground": "bgLight2",
        "titleBar.activeForeground": "txtDefault",
        "titleBar.inactiveBackground": "bgLight1",
        "titleBar.inactiveForeground": "txtDefault",

        "input.background": "bgDark2",
        "input.foreground": "txtDefault",
        "input.border": "highlightActive2",
        "input.placeholderForeground": "txtInactive",
        "inputOption.activeBackground": "highlightActive2",
        "inputOption.activeBorder": "highlightActive2",
        "inputOption.hoverBackground": "highlightInActive2",

        "button.background": "highlightActive2",
        "button.foreground": "txtLight",
        "button.hoverBackground": "highlightActive1",

        "badge.background": "highlightInActive1",

        "list.activeSelectionBackground": "highlightActive2",
        "list.activeSelectionForeground": "txtLight",
        "list.inactiveSelectionBackground": "highlightInActive2",
        "list.focusBackground": "highlightActive3",
        "list.focusOutline": "transparent",
        "list.hoverBackground": "highlightInactive3",

        "scrollbarSlider.activeBackground": "highlightActive2",
        "scrollbarSlider.hoverBackground": "highlightActive3",
        "scrollbarSlider.background": "highlightInActive2",

        "sideBar.background": "bgDark2",
        "sideBar.foreground": "txtDefault",
        "sideBar.border": "bgLight2",
        "sideBarSectionHeader.background": "bgLight2",

        "statusBar.foreground": "txtDefault",
        "statusBar.background": "bgLight2",
        "statusBarItem.hoverBackground": "highlightInActive2",

        "tab.inactiveBackground": "bgLight1",
        "tab.border": "bgLight2",
        "tab.activeBackground": "highlightActive3",
        "tab.activeBorder": "highlightActive2",

        "quickInput.background": "bgLight1",
        "quickInputList.focusBackground": "highlightActive2",
        "quickInputList.focusForeground": "txtLight",
        "quickInput.foreground": "txtDefault",

        "panel.background": "bgDark1",
        "panelTitle.activeForeground": "txtDefault"
    },
    "tokenColors": [
        /* General */
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "tkGrey1"
            }
        },
        {
            "name": "colors",
            "scope": [
                "constant.numeric.hex",
                "constant.rgb-value",
                "constant.other.color",
                "meta.function.color"
            ],
            "settings": {
                "foreground": "tkTurquoise1"
            }
        },
        {
            "name": "emphasis",
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "strong",
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "tkError"
            }
        },
        {
            "name": "true",
            "scope": "constant.language.boolean.true",
            "settings": {
                "foreground": "tkTrue"
            }
        },
        {
            "name": "false",
            "scope": "constant.language.boolean.false",
            "settings": {
                "foreground": "tkFalse"
            }
        },
        {
            "name": "punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "name": "string",
            "scope": "string",
            "settings": {
                "foreground": "tkYellowGreen1"
            }
        },
        {
            "name": "string special tokens",
            "scope": ["constant.character.escape", "constant.regexp"],
            "settings": {
                "foreground": "tkBabyBlue1"
            }
        },
        {
            "name": "constants",
            "scope": ["constant", "variable.other.constant"],
            "settings": {
                "foreground": "tkRedOrange2"
            }
        },
        {
            "name": "nullish values",
            "scope": ["constant.language.null", "constant.language.undefined"],
            "settings": {
                "foreground": "tkUndefined"
            }
        },
        {
            "name": "numbers",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "tkGreen1"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable"],
            "settings": {
                "foreground": "#d8af61"
            }
        },
        {
            "name": "Variables-readwrite",
            "scope": ["variable.other.readwrite"],
            "settings": {
                "foreground": "tkOrangeYellow2"
            }
        },
        {
            "name": "Objects",
            "scope": ["variable.other.object"],
            "settings": {
                "foreground": "tkOrangeYellow2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Object/Variable properties",
            "scope": [
                "variable.other.property",
                "meta.object.member",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "tkRed1"
            }
        },
        {
            "name": "storage: declarations (class, function, int, var, ect...)",
            "scope": ["storage"],
            "settings": {
                "foreground": "tkGreen2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars"
            ],
            "settings": {
                "foreground": "tkTurquoise1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "storage modifier (static, final, abstract, async, ect...)",
            "scope": ["storage.modifier"],
            "settings": {
                "foreground": "tkIndigo1"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword"],
            "settings": {
                "foreground": "tkIndigo1"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "name": "entity",
            "scope": ["entity.name.selector"],
            "settings": {
                "foreground": "tkFalse"
            }
        },
        {
            "name": "meta",
            "scope": ["meta"],
            "settings": {
                "foreground": "#949494"
            }
        },
        {
            "name": "markup",
            "scope": ["markup"],
            "settings": {
                "foreground": "#949494"
            }
        }
    ]
}
