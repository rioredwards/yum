  "tokenColors": [
    /* General */
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "tkGrey1"
      }
    },
    {
      "name": "colors",
      "scope": [
        "constant.numeric.hex",
        "constant.rgb-value",
        "constant.other.color",
        "meta.function.color"
      ],
      "settings": {
        "foreground": "tkTurquoise1"
      }
    },
    {
      "name": "emphasis",
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "strong",
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "tkFalse"
      }
    },
    {
      "name": "true",
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "tkTrue"
      }
    },
    {
      "name": "false",
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "tkFalse"
      }
    },
    {
      "name": "punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "tkYellowGreen1"
      }
    },
    {
      "name": "string special tokens",
      "scope": [
        "constant.character.escape",
        "constant.regexp",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "tkBabyBlue1"
      }
    },
    {
      "name": "constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "tkRedOrange2"
      }
    },
    {
      "name": "nullish values",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "tkUndefined"
      }
    },
    {
      "name": "numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "tkGreen1"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    {
      "name": "Variables-readwrite",
      "scope": ["variable.other.readwrite"],
      "settings": {
        "foreground": "tkOrangeYellow2"
      }
    },
    {
      "name": "Objects",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "tkOrangeYellow2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object/Variable properties",
      "scope": [
        "variable.other.property",
        "meta.object.member",
        "meta.object-literal.key",
        "meta.var.expr",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "tkRed1"
      }
    },
    {
      "name": "storage: declarations (class, function, int, var, ect...)",
      "scope": ["storage"],
      "settings": {
        "foreground": "tkGreen2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "tkTurquoise1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "storage modifier (static, final, abstract, async, ect...)",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "tkIndigo1"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.operator.expression.typeof"],
      "settings": {
        "foreground": "tkIndigo1"
      }
    },
    {
      "name": "New",
      "scope": ["keyword.operator.new"],
      "settings": {
        "foreground": "tkTrue"
      }
    },
    {
      "name": "Export & Import",
      "scope": ["keyword.control.export", "keyword.control.import"],
      "settings": {
        "foreground": "tkTeal1"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "entity",
      "scope": ["entity.name.selector"],
      "settings": {
        "foreground": "tkFalse"
      }
    },
    {
      "name": "meta",
      "scope": ["meta"],
      "settings": {
        "foreground": "tkWhite2"
      }
    },
