{
    "name": "yum",
    "colors": {
        "focusBorder": "#24a8e590",
        "foreground": "#cccccc",
        "disabledForeground": "#959595",
        "selection.background": "#1b94cd75",
        "descriptionForeground": "#959595",
        "icon.foreground": "#cccccc",

        "textLink.foreground": "#3da2d1",
        "textLink.activeForeground": "#5fbce8",

        "toolbar.hoverBackground": "#cecece37",
        "toolbar.activeBackground": "#24a8e590",

        "editor.foreground": "#cccccc",
        "editor.background": "#272727",
        "editorGutter.background": "#2F2F2F",
        "editor.lineHighlightBackground": "#1b95cd24",
        "editorWidget.background": "#2F2F2F",
        "editorSuggestWidget.selectedBackground": "#1b94cd75",
        "editorSuggestWidget.highlightForeground": "#e9323296",
        "editorHoverWidget.border": "#474747",
        "editorHoverWidget.background": "#2F2F2F",
        "editorHoverWidget.foreground": "#cccccc",
        "editorStickyScroll.background": "#272727",

        "activityBar.background": "#3a3a3a",
        "activityBar.foreground": "#3da2d1",
        "activityBar.activeBackground": "#474747",
        "activityBar.inactiveForeground": "#959595",
        "activityBarBadge.background": "#3da2d1",
        "activityBarBadge.foreground": "#f8f8f8",

        "titleBar.activeBackground": "#3a3a3a",
        "titleBar.activeForeground": "#cccccc",
        "titleBar.inactiveBackground": "#2F2F2F",
        "titleBar.inactiveForeground": "#cccccc",

        "input.background": "#272727",
        "input.foreground": "#cccccc",
        "input.border": "#1b94cd75",
        "input.placeholderForeground": "#959595",
        "inputOption.activeBackground": "#1b94cd75",
        "inputOption.activeBorder": "#1b94cd75",
        "inputOption.hoverBackground": "#cecece29",

        "button.background": "#1b94cd75",
        "button.foreground": "#f8f8f8",
        "button.hoverBackground": "#24a8e590",

        "badge.background": "#cecece37",

        "list.activeSelectionBackground": "#1b94cd75",
        "list.activeSelectionForeground": "#f8f8f8",
        "list.inactiveSelectionBackground": "#cecece29",
        "list.focusBackground": "#1b95cd42",
        "list.focusOutline": "#00000000",
        "list.hoverBackground": "#cecece1a",

        "scrollbarSlider.activeBackground": "#1b94cd75",
        "scrollbarSlider.hoverBackground": "#1b95cd42",
        "scrollbarSlider.background": "#cecece29",

        "sideBar.background": "#272727",
        "sideBar.foreground": "#cccccc",
        "sideBar.border": "#3a3a3a",
        "sideBarSectionHeader.background": "#3a3a3a",

        "statusBar.foreground": "#cccccc",
        "statusBar.background": "#3a3a3a",
        "statusBarItem.hoverBackground": "#cecece29",

        "tab.inactiveBackground": "#2F2F2F",
        "tab.border": "#3a3a3a",
        "tab.activeBackground": "#1b95cd42",
        "tab.activeBorder": "#1b94cd75",

        "quickInput.background": "#2F2F2F",
        "quickInputList.focusBackground": "#1b94cd75",
        "quickInputList.focusForeground": "#f8f8f8",
        "quickInput.foreground": "#cccccc",

        "panel.background": "#212121",
        "panelTitle.activeForeground": "#cccccc"
    },
    "tokenColors": [
        /* General */
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8c8c8c"
            }
        },
        {
            "name": "colors",
            "scope": [
                "constant.numeric.hex",
                "constant.rgb-value",
                "constant.other.color",
                "meta.function.color"
            ],
            "settings": {
                "foreground": "#6ec5c2"
            }
        },
        {
            "name": "emphasis",
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "strong",
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "tkError"
            }
        },
        {
            "name": "true",
            "scope": "constant.language.boolean.true",
            "settings": {
                "foreground": "#34b73c"
            }
        },
        {
            "name": "false",
            "scope": "constant.language.boolean.false",
            "settings": {
                "foreground": "#d33223"
            }
        },
        {
            "name": "punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "name": "string",
            "scope": "string",
            "settings": {
                "foreground": "#9bb572"
            }
        },
        {
            "name": "string special tokens",
            "scope": ["constant.character.escape", "constant.regexp"],
            "settings": {
                "foreground": "#7bc4df"
            }
        },
        {
            "name": "constants",
            "scope": ["constant", "variable.other.constant"],
            "settings": {
                "foreground": "#CF6A4C"
            }
        },
        {
            "name": "nullish values",
            "scope": ["constant.language.null", "constant.language.undefined"],
            "settings": {
                "foreground": "#e26868"
            }
        },
        {
            "name": "numbers",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#62b464"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable"],
            "settings": {
                "foreground": "#E79F56"
            }
        },
        {
            "name": "Variables-readwrite",
            "scope": ["variable.other.readwrite"],
            "settings": {
                "foreground": "#E79F56"
            }
        },
        {
            "name": "Objects",
            "scope": ["variable.other.object"],
            "settings": {
                "foreground": "#E79F56",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Object/Variable properties",
            "scope": [
                "variable.other.property",
                "meta.object.member",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "storage: declarations (class, function, int, var, ect...)",
            "scope": ["storage"],
            "settings": {
                "foreground": "#509151",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars"
            ],
            "settings": {
                "foreground": "#6ec5c2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "storage modifier (static, final, abstract, async, ect...)",
            "scope": ["storage.modifier"],
            "settings": {
                "foreground": "#7485d0"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword"],
            "settings": {
                "foreground": "#7485d0"
            }
        },
        {
            "name": "Export & Import",
            "scope": ["keyword.control.export", "keyword.control.import"],
            "settings": {
                "foreground": "#86D3BB"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "name": "entity",
            "scope": ["entity.name.selector"],
            "settings": {
                "foreground": "#d33223"
            }
        },
        {
            "name": "meta",
            "scope": ["meta"],
            "settings": {
                "foreground": "#c8c8c8"
            }
        },
        // JAVASCRIPT ----------------------------------------
        {
            "scope": "string.quoted.double.js",
            "settings": {
                "foreground": "#509151"
            }
        },
        // REACT.JS ----------------------------------------
        {
            "scope": "entity.name.tag.js",
            "settings": {
                "foreground": "#CF6A4C"
            }
        },
        {
            "scope": "support.class.component.js",
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "scope": "entity.other.attribute-name.js",
            "settings": {
                "foreground": "#E79F56"
            }
        },
        // HTML ----------------------------------------
        {
            "scope": "string.quoted.double.html",
            "settings": {
                "foreground": "#9bb572"
            }
        },
        {
            "name": "HTML Tag",
            "scope": ["entity.name.tag.html"],
            "settings": {
                "foreground": "#CF6A4C"
            }
        },
        {
            "name": "HTML punctuation",
            "scope": [
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#53a992"
            }
        },
        {
            "name": "HTML attribute",
            "scope": ["entity.other.attribute-name.html"],
            "settings": {
                "foreground": "#E79F56"
            }
        },
        // CSS ----------------------------------------
        {
            "name": "CSS Selector",
            "scope": [
                "entity.name.tag.css",
                "entity.other.attribute-name.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#E79F56"
            }
        },
        {
            "name": "CSS properties",
            "scope": [
                "support.type.property-name.css",
                "support.type.vendored.property-name.css"
            ],
            "settings": {
                "foreground": "#6ec5c2"
            }
        },
        {
            "name": "CSS functions",
            "scope": [
                "source.css support.function.transform",
                "source.css support.function.timing-function",
                "source.css support.function.misc"
            ],
            "settings": {
                "foreground": "#62b464",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS property values",
            "scope": [
                "support.property-value",
                "constant.numeric.css",
                "constant.rgb-value",
                "support.property-value.scss",
                "constant.rgb-value.scss",
                "meta.property-value.css",
                "support.constant.property-value.css"
            ],
            "settings": {
                "foreground": "#c8c8c8"
            }
        },
        {
            "name": "CSS units",
            "scope": ["keyword.other.unit"],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "CSS variables",
            "scope": ["variable.css", "variable.argument.css"],
            "settings": {
                "foreground": "#d67c6e"
            }
        },
        {
            "name": "CSS Important",
            "scope": ["keyword.other.important.css"],
            "settings": {
                "foreground": "#34b73c",
                "fontStyle": "bold"
            }
        },
        // JSON ----------------------------------------
        {
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "#c8c8c8"
            }
        },
        {
            "name": "JSON Key - Level 0, 4, 8",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#53a992"
            }
        },
        {
            "name": "JSON Key - Level 1, 5",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#E79F56"
            }
        },
        {
            "name": "JSON Key - Level 2, 6",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#86D3BB"
            }
        },
        {
            "name": "JSON Key - Level 3, 7",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#d67c6e"
            }
        },
        // MARKDOWN/MARKUP ----------------------------------------
        {
            "name": "markup",
            "scope": ["markup"],
            "settings": {
                "foreground": "#c8c8c8"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "string.other.link.title.markdown",
            "settings": {
                "foreground": "#efefef",
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#bd92ea"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#CF6A4C"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E79F56"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#62b464"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#CF6A4C"
            }
        },
        {
            "scope": "markup.punctuation.quote.beginning",
            "settings": {
                "foreground": "#7bc4df"
            }
        },
        {
            "scope": "markup.punctuation.list.beginning",
            "settings": {
                "foreground": "#7bc4df"
            }
        },
        {
            "scope": ["markup.inline.raw", "markup.fenced_code.block"],
            "settings": {
                "foreground": "#53a992"
            }
        }
    ]
}
