  "tokenColors": [
    /* General */
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8c8c8c"
      }
    },
    {
      "name": "colors",
      "scope": [
        "constant.numeric.hex",
        "constant.rgb-value",
        "constant.other.color",
        "meta.function.color"
      ],
      "settings": {
        "foreground": "#6bc1be"
      }
    },
    {
      "name": "emphasis",
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "strong",
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#d33223"
      }
    },
    {
      "name": "true",
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "#34b73c"
      }
    },
    {
      "name": "false",
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "#d33223"
      }
    },
    {
      "name": "punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#9bb572"
      }
    },
    {
      "name": "string special tokens",
      "scope": [
        "constant.character.escape",
        "constant.regexp",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#7bc4df"
      }
    },
    {
      "name": "constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#CF6A4C"
      }
    },
    {
      "name": "nullish values",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#e26868"
      }
    },
    {
      "name": "numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#62b464"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#E79F56"
      }
    },
    {
      "name": "Variables-readwrite",
      "scope": ["variable.other.readwrite"],
      "settings": {
        "foreground": "#E79F56"
      }
    },
    {
      "name": "Objects",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "#E79F56",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object/Variable properties",
      "scope": [
        "variable.other.property",
        "meta.object.member",
        "meta.object-literal.key",
        "meta.var.expr",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "name": "storage: declarations (class, function, int, var, ect...)",
      "scope": ["storage"],
      "settings": {
        "foreground": "#509151",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars"
